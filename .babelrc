{
    "plugins": [
        [
            "import",
            {
                "libraryName": "antd",
                "style": "css"
            }
        ],
        [
            "module-resolver",
            {
                "root": ["."],
                "alias": {
                    "styles": "scss/"
                },
                "cwd": "babelrc"
            }

        ],
        [
            "wrap-in-js",
            {
                "extensions": ["css$", "scss$"]
            }
        ]

    ],
    "presets": ["next/babel"],
    "ignore": []
}